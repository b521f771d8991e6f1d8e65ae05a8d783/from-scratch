{
	"name": "webui",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"reset-project": "node ./scripts/reset-project.js",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"lint": "expo lint",
		"build:web": "expo export --platform web && npx shx rm -rf ../../generated/web-dist && npx shx mkdir -p ../../generated/web-dist && npx shx cp -r dist ../../generated/web-dist/frontend",
		"build:android:debug": "export NODE_ENV=development && npx expo prebuild -p android && cd android && ./gradlew assembleRelease",
		"build:android:release": "export NODE_ENV=production && npx expo prebuild -p android && cd android && ./gradlew assembleRelease"
	},
	"dependencies": {
		"@dotenvx/dotenvx": "^1.44.0",
		"@expo/vector-icons": "^14.1.0",
		"@react-native-async-storage/async-storage": "2.1.2",
		"@react-navigation/bottom-tabs": "^7.3.10",
		"@react-navigation/elements": "^2.3.8",
		"@react-navigation/native": "^7.1.6",
		"@reduxjs/toolkit": "^2.5.1",
		"@stylistic/eslint-plugin-js": "^4.2.0",
		"expo": "~53.0.11",
		"expo-auth-session": "^6.2.0",
		"expo-blur": "~14.1.5",
		"expo-constants": "~17.1.6",
		"expo-font": "~13.3.1",
		"expo-haptics": "~14.1.4",
		"expo-image": "~2.3.0",
		"expo-linking": "~7.1.5",
		"expo-localization": "~16.1.6",
		"expo-router": "~5.1.0",
		"expo-splash-screen": "~0.30.9",
		"expo-status-bar": "~2.2.3",
		"expo-symbols": "~0.4.5",
		"expo-system-ui": "~5.0.8",
		"expo-web-browser": "~14.1.6",
		"keycloak-js": "^26.2.0",
		"nativewind": "^4.1.23",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-native": "0.79.3",
		"react-native-gesture-handler": "~2.24.0",
		"react-native-reanimated": "~3.17.4",
		"react-native-safe-area-context": "^5.4.0",
		"react-native-screens": "~4.11.1",
		"react-native-svg": "^15.12.0",
		"react-native-web": "~0.20.0",
		"react-native-webview": "13.13.5",
		"react-native-worklets": "^0.6.0",
		"react-redux": "^9.2.0",
		"redux": "^5.0.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.0"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@types/react": "~19.0.10",
		"eslint-config-expo": "~9.2.0",
		"typescript": "~5.8.3"
	},
	"private": true
}
